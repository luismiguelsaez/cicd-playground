pipeline {
  agent any
  triggers {
    GenericTrigger(
     genericVariables: [
      [key: 'POST_DATA', value: '$']
     ],

     //causeString: 'Triggered on $BRANCH_WH',

     token: 'dummy',
     tokenCredentialId: '',

     printContributedVariables: true,
     printPostContent: true,

     silentResponse: false,

     //regexpFilterText: '$BRANCH_WH',
     //regexpFilterExpression: 'refs/heads/' + BRANCH_NAME
    )
  }
  stages {
    stage('Debugging build') {
      steps {
        sh "echo $POST_DATA"
      }
    }
  }
}